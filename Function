#include <iostream>

using namespace std;

// Deklarasi function dengan parameter
int tambah (int a, int b){
  return a + b;
}

int main() {

  int x = 5, y = 7, hasil;
  
  // Memanggil function tambah dan menyimpan hasilnya ke dalam variabel hasil 
  hasil = tambah(x, y);
  
  // Menampilkan hasil
  cout << "Hasil penjumlahan << x << " dan " << y << " adalah " << hasil << endl;
  
  return 0;
}
